{
  "name": "markdown-table-editor",
  "displayName": "Markdown Table Editor",
  "description": "Edit Markdown tables with a spreadsheet-like interface",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdownTableEditor.openEditor",
        "title": "Open Table Editor",
        "category": "Markdown Table Editor"
      },
      {
        "command": "markdownTableEditor.createTable",
        "title": "Create New Table",
        "category": "Markdown Table Editor"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "markdownTableEditor.openEditor",
          "when": "resourceExtname == .md",
          "group": "navigation"
        },
        {
          "command": "markdownTableEditor.createTable",
          "when": "resourceExtname == .md",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@types/mocha": "^9.1.1",
    "@types/glob": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.0",
    "mocha": "^10.1.0",
    "glob": "^8.0.3"
  },
  "dependencies": {
    "markdown-it": "^13.0.1"
  }
}